{
    "name": "@flocasts/flo-axios-digest",
    "version": "0.1.1",
    "description": "This repo exists because the Axios library does not support digest authentication.",
    "authors": "Austin See <austin.see@flosports.tv>",
    "private": false,
    "main": "dist/index.js",
    "repository": "https://github.com/flocasts/flo-axios-digest",
    "engines": {
        "node": "^16.17.0",
        "npm": "^8.15.0",
        "yarn": "please-use-npm"
    },
    "scripts": {
        "build": "npm run clean && tsc",
        "build:package": "npm run clean && tsc -p tsconfig.build.json",
        "clean": "rm -rf dist",
        "lint": "eslint --fix",
        "test": "concurrently --kill-others \"node ./test-server/index.js\" \"npm run build && jest\"",
        "test:server": "node ./test-server/index.js",
        "semantic-release": "npx --no-install semantic-release"
    },
    "devDependencies": {
        "@types/jest": "^29.2.2",
        "@types/node": "^14.18.33",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "concurrently": "^7.5.0",
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "http-auth": "^4.2.0",
        "prettier": "^2.7.1",
        "ts-jest": "^29.0.3",
        "typescript": "^4.8.4"
    },
    "peerDependencies": {
        "axios": "^0.27.0"
    },
    "license": "ISC",
    "keywords": [
        "FloSports",
        "axios",
        "digest-auth",
        "digest"
    ],
    "publishConfig": {
        "@flocasts:registry": "https://npm.pkg.github.com"
    }
}
